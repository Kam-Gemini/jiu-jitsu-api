<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Comments</title>
</head>
<body>
    <h1>Comments</h1>
    <ul>
        <% divisions.forEach((division) => { %>
           <% division.comments.forEach((comment) => { %> 
         <li>
            <%= division.belt %> Belt comment: <%= comment.content %> by <%= comment.user.username %>
        </li>
        <% }) %>
        <% }) %>
    </ul>
</body>
</html>